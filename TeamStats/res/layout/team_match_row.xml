        android:layout_height="wrap_content"
        android:text="TextView"  
        android:padding="5dp"/>

    <TextView
        android:id="@+id/awayScoreTextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"  
        android:padding="5dp"/>

    <TextView
        android:id="@+id/resultTextView"
        android:layout_width="wrap_content"
